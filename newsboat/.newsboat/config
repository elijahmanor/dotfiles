include ~/.newsboat/dark

# general settings
# auto-reload yes
# max-items 50

# unbind keys
unbind-key h
unbind-key j
unbind-key k
unbind-key l
unbind-key g # bound to `sort` by default
unbind-key G # bound to `rev-sort` by default

# vim style bindings
bind-key h quit
bind-key j down
bind-key k up
bind-key l open
bind-key g home
bind-key G end
# bind-key ^U halfpageup
# bind-key ^D halfpagedown
